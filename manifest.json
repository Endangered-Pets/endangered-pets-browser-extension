{
    "manifest_version": 2,
    "name": "Endangered Pets Browser Extension",
    "short_name": "EP",
    "version": "1.0.0",
    "description": "A chrome extension lets you have your very own virtual pets",
    "author": "Endangered Pets",
    "homepage_url": "http://github.com/rossmccrann",
    "chrome_url_overrides": {
      "newtab": "index.html"
    },
    "background": {
      "scripts": ["src/js/background.js"]
    },
    "permissions": [
      "storage",
      "tabs",
      "activeTab"
    ]
}
